from cuit import verificar_cuit

# Números de CUIT/CUIL para probar
cuits = [
    '30-56225215-7', 
    '27-38426233-9',
    '23-33016244-9',
    'A0-330B6244-C',
    '30562252157'  ,
    '305622157'    ,
]

for cuit in cuits:
    if verificar_cuit(cuit):
        print(f'{cuit} es un CUIT/CUIL válido')
    else:
        print(f'{cuit} no es un CUIT/CUIL válido')
